package classes;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.Map;

public class AnsweredQuizz {
	Quizz quizz;
	Map<User,ArrayList<Integer>> answers;
	public AnsweredQuizz(Quizz q){
		quizz=q;
		answers=new HashMap<User, ArrayList<Integer>>();
	}
	public void addUserAnswers(User user, ArrayList<Integer> a){answers.put(user,a);}
}
